    <div class="discription">1.人名漢字で異体字の場合は氏名備考欄にその旨を記入してください。<br>
      2.フリガナは全角カタカナ、電話番号、メールアドレスは半角で記入してください。<br>
      3.監督と引率者が同一の場合も引率者欄にも記入してください。</div>
    <div class="f_box">


{foreach from=$fields_info item=info}
{if $info.kind != ''}
      <div class="midasi">{$info.name}</div>



    <tr>
{assign var="ref_field" value="ref_`$info.field`"}
{assign var="edit_field" value=$info.field}
        <td class="normal"{if $info.language == 'mono'} colspan="2"{/if}>
{if $info.kind == 'text'}
            <input id="kanri_eng" type="text" name="{$ref_field}" value="{$smarty.session.p.$ref_field}" class="text">
{if $info.language == 'multi'}
        </td>
        <td class="normal">
            <input id="kanri_eng" type="text" name="{$edit_field}" value="{$smarty.session.p.$edit_field}" class="text">
{/if}
{elseif $info.kind == 'textarea'}
            <textarea id="kanri_eng" name="{$ref_field}" rows="8">{$smarty.session.p.$ref_field}</textarea>
{if $info.language == 'multi'}
        </td>
        <td class="normal">
            <textarea id="kanri_eng" name="{$edit_field}" rows="8">{$smarty.session.p.$edit_field}</textarea>
{/if}
{elseif $info.kind == 'check'}
			{html_checkboxes name=$ref_field options=$info.sel1 selected=$smarty.session.p.$ref_field separator='<br />'}
{if $info.language == 'multi'}
        </td>
			{html_checkboxes name=$edit_field options=$info.sel2 selected=$smarty.session.p.$edit_field separator='<br />'}
{/if}
{elseif $info.kind == 'check_one'}
			{html_checkboxes name=$ref_field options=$info.sel1 selected=$smarty.session.p.$ref_field separator='<br />'}
{if $info.language == 'multi'}
        </td>
			{html_checkboxes name=$edit_field options=$info.sel2 selected=$smarty.session.p.$edit_field separator='<br />'}
{/if}
{elseif $info.kind == 'select'}
            {html_options name=$ref_field options=$info.sel1 selected=$smarty.session.p.$ref_field}
{if $info.language == 'multi'}
        </td>
        <td class="normal">
            {html_options name=$edit_field options=$info.sel2 selected=$smarty.session.p.$edit_field}
{/if}
{elseif $info.kind == 'radio'}
			{html_radios name=$ref_field options=$info.sel1 selected=$smarty.session.p.$ref_field separator='<br />'}
{if $info.language == 'multi'}
        </td>
        <td class="normal">
			{html_radios name=$edit_field options=$info.sel2 selected=$smarty.session.p.$edit_field separator='<br />'}
{/if}
{elseif $info.kind == 'zip'}
{assign var="ref_field2" value="ref_`$info.field`2"}
            <input id="{$ref_field}" type="text" name="{$ref_field}" value="{$smarty.session.p.$ref_field}" class="text zip">
            -<input id="{$ref_field2}" type="text" name="{$ref_field2}" value="{$smarty.session.p.$ref_field2}" class="text zip">
            <input id="{$info.field}_input" type="button" name="{$info.field}_input" value="住所自動入力" class="" onClick="zip_search();">
{if $info.language == 'multi'}
        </td>
        <td class="normal">
            <input id="{$ref_field}" type="text" name="{$ref_field}" value="{$smarty.session.p.$ref_field}" class="text zip">
            -<input id="{$ref_field2}" type="text" name="{$ref_field2}" value="{$smarty.session.p.$ref_field2}" class="text zip">
{/if}
{elseif $info.kind == 'admin_zip'}
{assign var="ref_field2" value="ref_`$info.field`2"}
            <input id="{$ref_field}" type="text" name="{$ref_field}" value="{$smarty.session.p.$ref_field}" class="text zip">
            -<input id="{$ref_field2}" type="text" name="{$ref_field2}" value="{$smarty.session.p.$ref_field2}" class="text zip">
{*
            <input id="{$info.field}_input" type="button" name="{$info.field}_input" value="住所自動入力" class="" onClick="admin_zip_search();">
*}
{if $info.language == 'multi'}
        </td>
        <td class="normal">
            <input id="{$ref_field}" type="text" name="{$ref_field}" value="{$smarty.session.p.$ref_field}" class="text zip">
            -<input id="{$ref_field2}" type="text" name="{$ref_field2}" value="{$smarty.session.p.$ref_field2}" class="text zip">
{/if}
{elseif $info.kind == 'tel'}
{assign var="ref_field2" value="ref_`$info.field`2"}
            +81(0)<input id="kanri_eng" type="text" name="{$ref_field}" value="{$smarty.session.p.$ref_field}" class="text">
{elseif $info.kind == 'party'}
{assign var="ref_field2" value="ref_`$info.field`2"}
            <input type="radio" name="{$ref_field}_check" value="1" {if $smarty.session.p.$ref_field>0}checked="checked" {/if}/>{$info.sel1[1][0]}<input id="kanri_eng" type="text" name="{$ref_field}" value="{if $smarty.session.p.$ref_field>0}{$smarty.session.p.$ref_field}{/if}" class="text zip">{$info.sel1[1][1]}<input type="radio" name="{$ref_field}_check" value="0" {if $smarty.session.p.$ref_field==0}checked="checked" {/if}/>{$info.sel1[0][0]}
{elseif $info.kind == 'seating'}
{assign var="ref_field2" value="ref_`$info.field`2"}
            {$info.sel1[1][0]}<input id="kanri_eng" type="text" name="{$ref_field}" value="{$smarty.session.p.$ref_field}" class="text zip">{$info.sel1[1][1]}<input id="kanri_eng" type="text" name="{$ref_field2}" value="{$smarty.session.p.$ref_field2}" class="text zip">{$info.sel1[1][2]}
{/if}
        </td>
    </tr>
{/if}
{/foreach}





